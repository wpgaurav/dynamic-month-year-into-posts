{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "dmyip/modified-date",
  "version": "1.7.1",
  "title": "Modified Date",
  "category": "widgets",
  "icon": "update",
  "description": "Display the post's last modified date dynamically.",
  "keywords": [
    "date",
    "modified",
    "updated",
    "edited",
    "time"
  ],
  "textdomain": "dynamic-month-year-into-posts",
  "usesContext": [
    "postId",
    "postType"
  ],
  "attributes": {
    "format": {
      "type": "string",
      "default": ""
    },
    "prefix": {
      "type": "string",
      "default": ""
    },
    "suffix": {
      "type": "string",
      "default": ""
    }
  },
  "supports": {
    "html": false,
    "align": [
      "left",
      "center",
      "right"
    ],
    "typography": {
      "fontSize": true,
      "lineHeight": true
    },
    "color": {
      "text": true,
      "background": true
    },
    "spacing": {
      "margin": true,
      "padding": true
    }
  },
  "editorScript": "file:./index.js",
  "render": "file:./render.php"
}